	xmlns:mvc="sap.ui.core.mvc">
	<FlexibleColumnLayout
		id="mainLayout"
		stateChange="onStateChanged"
		backgroundDesign="Solid">
		<beginColumnPages>
			<mvc:XMLView id="beginView" viewName="zmm.vehicle.catalog.view.Category"/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView id="midColumnView" viewName="zmm.vehicle.catalog.view.Vehicle"/>
		</midColumnPages>
	</FlexibleColumnLayout>
</mvc:View>